<h2>Edit Party Information:</h2>

<form action="/students/<%= @student.id%>" method="post">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  <input type="text" name="student_name" value="<%= @student.student_name %>"><br>
  <input type="date" name="student_birthday" value="<%= @student.student_birthday %>"><br>
  <input type="text" name="venue" value="<%= @party.venue %>"><br>
  <input type="submit" value="submit">
</form>
